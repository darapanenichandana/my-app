from textblob import TextBlob

def analyze(text):
    blob = TextBlob(text)   
    sentiment = blob.sentiment
    polarity = sentiment.polarity
    if polarity > 0:
        category = "positive"
    elif polarity <0:
        category = "Negative"
    else:
        category = "Neutral"
    return category

text = "i eat icecream"
result = analyze(text)
print(f"sentiment:{result}")